    "/api/users": {
      "post": {
        "consumes": [
          "application/json"
        ],
        "description": "Creates a new user",
        "operationId": "GreenhouseWeb.UserController.create",
        "parameters": [
          {
            "description": "The user details",
            "in": "body",
            "name": "user",
            "required": false,
            "schema": {
              "$ref": "#/definitions/UserRequest"
            },
            "x-example": {
              "user": {
                "birthday": "15/Nov/2000",
                "email": "gomez.c.orlando@gmail.com",
                "image": "/home/orlando/Pictures/avatar_mujer.png",
                "is_admin": "true",
                "language": "spanish",
                "lastname": "Resendiz",
                "middlename": "KArina",
                "name": "Alondra",
                "phone": "+524271145588",
                "second_lastname": "Alvarez",
                "timezone": "UTC/GMT -6 hours Mexico Guadalajara"
              }
            }
          }
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "201": {
            "description": "User created OK",
            "schema": {
              "$ref": "#/definitions/UserResponse"
            }
          },
          "422": {
            "description": "Unprocessable Entity",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "summary": "Create user",
        "tags": [
          "User"
        ]
      }
    },
